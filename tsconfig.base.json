{
	"compilerOptions": {
		"composite": true,
		"incremental": true,
		"target": "ES2021",
		"module": "commonjs",
		"lib": ["es2021"],
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	}
}
